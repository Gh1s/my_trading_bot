def get_data():
    df = yf.download(
        tickers = "EURUSD=X",
        period = "12d",
        interval = "5m",
        group_by = "ticker",
        auto_adjust = True,
        threads = True,
        proxy = None
    )
    df = df.dropna()
    df.index = df.index.strftime('%Y/%m/%d %H:%M:%S')
    return df


